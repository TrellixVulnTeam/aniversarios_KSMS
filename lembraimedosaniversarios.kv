ScreenManager:
    Main:
        name:'Principal'

<MyPopup@MDDialog>:
    id: mypop
    name:"MyPopup"
    auto_dismiss: False
    Button:
        text: 'Close me!'
        on_release: root.dismiss()

<Main>:
    on_enter: root.printar_label()
    AnchorLayout:
        MDLabel:
            id:aniversariododia
            halign:'center'

        BoxLayout:
            padding:10
            MDTextButton:
                id: sair
                text: 'Sair'
                on_press: app.stop()


